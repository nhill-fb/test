# The myshopify.com URL to your Shopify store
SHOPIFY_STORE=mydomain.myshopify.com
# The API key from a Private App
SHOPIFY_API_KEY=
# The API password generated from a Private App
SHOPIFY_API_PASSWORD=

# The ID of the theme you wish to upload files too 
THEME_ID= 

# A list of file patterns to ignore, with each list item seperated by ':' -- /config/settings_data.json is automatically ignored already
IGNORE_FILES=

# Port number for BrowserStack to use, default is 8080
PORT=
